@article{Liang2003,
	abstract = {A series of new PPV oligomers contg. 8-substituted quinoline, 2,2'-(arylenedivinylene)bis-8-quinoline derivs., were designed and synthesized via a Knoevenagel condensation reaction of quinaldine, 8-hydroxy- or 8-methoxy-quinaldine with arom. dialdehydes. These PPV oligomers were characterized by 1H and 13C-NMR, X-ray diffraction, elemental anal., UV-visible and fluorescence spectroscopies. The X-ray diffraction investigation showed that there are intermol. pi...pi interactions in the solid state in 1 and 3. The optical and photoluminescent properties study demonstrated that the emission color of the resulting materials varies from blue to yellow and is dependent on the substituents (pi-donor and pi-acceptor groups) on both sides of the conjugated mols. and the arom. core in the middle of the conjugated backbones. The electroluminescent devices using compds. 1-4 as the emitters and electron-transporting layers were fabricated with the structure ITO/NPB/emitter/LiF/Al. The best device performance with the max. brightness of 5530 cd m-2 and the luminous efficiency of 2.4 cd A-1 is achieved by using compd. 4, with intramol. charge transfer character, as the emitter; these values represent a more than 5-fold improvement in brightness and efficiency compared to compd. 3 without methoxy groups on the Ph rings. [on SciFinder (R)]},
	author = {Liang, Fushun and Chen, Jiangshan and Cheng, Yanxiang and Wang, Lixiang and Ma, Dongge and Jing, Xiabin and Wang, Fosong},
	doi = {10.1039/b210408c},
	file = {:D$\backslash$:/{\~{}}gakim/Скачанные статьи/Mendeley/Journal of Materials Chemistry/Liang et al. - 2003 - Synthesis, characterization, photoluminescent and electroluminescent properties of new conjugated 2,2-(arylenedivi.pdf:pdf},
	issn = {0959-9428},
	journal = {J. Mater. Chem.},
	mendeley-groups = {Фотоника,Стирилхиназолиноны},
	number = {6},
	pages = {1392},
	title = {{Synthesis, characterization, photoluminescent and electroluminescent properties of new conjugated 2,2?-(arylenedivinylene)bis-8-substituted quinolines}},
	volume = {13},
	year = {2003}
}

